            <field type="float" name="lat"/>
            <field type="float" name="lng"/>
            <field type="float" name="alt"/>
        </message>
        <message id="209" name="TASK_CONFIRM_RESPONSE">
            <description>Task confirm response</description>
            <field type="uint8_t" name="rid"/>
            <field type="uint16_t" name="task_id"/>
            <field type="uint8_t" name="confirmed"/>
