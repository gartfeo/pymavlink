<?xml version="1.0"?>
<mavlink>
    <include>common.xml</include>
    <version>2</version>
    <messages>
        <message id="201" name="CHECK_IN">
            <description>Check in</description>
            <field type="uint8_t" name="sid"/>
        </message>
        <message id="202" name="CHECK_OUT">
            <description>Check out</description>
            <field type="uint8_t" name="sid"/>
            <field type="float" name="lat"/>
            <field type="float" name="lng"/>
            <field type="float" name="alt"/>
        </message>
        <message id="203" name="LOG_STATUS">
            <description>Log status</description>
            <field type="uint8_t" name="sid"/>
            <field type="uint8_t" name="rid"/>
